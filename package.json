{
  "private": true,
  "packageManager": "pnpm@7.19.0",
  "scripts": {
    "dev": "nuxi dev",
    "build": "nuxi build",
    "generate": "nuxi generate",
    "preview": "nuxi preview",
    "lint": "eslint .",
    "type": "module",
    "lint:fix": "eslint . --fix",
    "format": "prettier \"**/*.{css,html,json,md,ts,tsx,vue,yml}\" --write --ignore-path .gitignore",
    "postinstall": "simple-git-hooks && nuxi prepare"
  },
  "lint-staged": {
    "**/*.{ts,vue}": "eslint --cache --ignore-path .gitignore"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "devDependencies": {
    "@nuxt/eslint-config": "^0.1.1",
    "@nuxtjs/device": "^3.0.0",
    "@nuxtjs/style-resources": "^1.2.1",
    "@sindresorhus/slugify": "^2.1.1",
    "@vueuse/nuxt": "^9.8.2",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.5.0",
    "lint-staged": "^13.1.0",
    "medium-zoom": "^1.0.8",
    "nuxt": "^3.0.0",
    "nuxt-kql": "^0.25.4",
    "postcss": "^8.4.21",
    "postcss-100vh-fix": "^1.0.2",
    "prettier": "^2.8.1",
    "pug": "^3.0.2",
    "pug-plain-loader": "^1.1.0",
    "sass": "^1.57.1",
    "sass-loader": "^10.4.1",
    "simple-git-hooks": "^2.8.1",
    "typescript": "^4.9.4",
    "vue-tsc": "^1.0.16"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "vue"
      ]
    }
  },
  "dependencies": {
    "@openafg/nuxt-fullpage": "^1.0.5",
    "@types/node": "^18.11.18",
    "nuxt-swiper": "^0.1.6",
    "vue-scroll-snap": "^0.1.11"
  }
}
